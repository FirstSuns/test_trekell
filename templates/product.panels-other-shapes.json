      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "<div class=\"product_table\"><div class=\"page-width page-width collection-product-list product-variant-list\">\n  <div class=\"grid grid--uniform grid--uniform-row-gutters large-row\">\n    <div id=\"{{t | handleize}}\" class=\"product-item grid__item\">\n      <div class=\"product-variant-section grid__item\">\n        <table class=\"variant-table  table--maxwidth\">\n          <thead>\n            <tr>\n              <th>Description<\/th>\n              <th>Size<\/th>       \n              <th>Profile<\/th>\n              <th>MSRP<\/th>\n              <th>Price<\/th>\n              <th><\/th>\n              <th>Quantity<\/th>\n            <\/tr>\n          <\/thead>\n          <tbody>\t\t\t\n            {% for variant in product.variants %} \n            <tr id=\"{{ variant.id }}\"> \n              <td>{{ variant.metafields.custom_fields[\"variant_description\"] }}<\/td>\n              <td>{{variant.option1}}<\/td>                    \n              <td>{{ variant.metafields.custom_fields[\"profile\"] }}<\/td>\n              <td>${{ variant.metafields.custom_fields[\"msrp\"] }}<\/td>\n              {%- capture wsprice -%}{% include 'ws-item-price', item: variant, money: false %}{%- endcapture -%}{%- assign wsprice = wsprice | plus: 0 -%}\n              <td>{{ wsprice | money }}<\/td>\n              <td>\n             <!-- Wishlist Hero Button -->\n                {% assign productNameSuffix = variant.options | join: \" \/ \" | prepend: \" : \"  %}\n              {% include 'wishlisthero-collection-product' with product: product, forceSelectedVariant: variant, productNameSuffix: productNameSuffix , buttonClass: \"wishlisthero-button-in-product-table\"  %}\n              <!--\/ Wishlist Hero Button -->\n              <\/td>\n              \n              <td {%if variant.metafields.custom_fields[\"warning_symbol\"] == 1 %} class=\"yellow-mark\"{%endif%}>\n               \n                  {%comment%}\n                  <div class=\"qty-wrapper\">\n                    <div class=\"qty-left\">\n                      <div class=\"span-absolute\"><\/div>\n                      <input  {% if variant.inventory_management == 'shopify' and variant.inventory_policy == 'deny' %} data-max=\"{{ variant.inventory_quantity }}\"{% endif %} data-price=\"{{ wsprice | money }}\" type=\"text\" name=\"updates[]\" field='{{ variant.id }}'  id=\"updates_{{ variant.id }}\" class=\"stickyquantity\" value=\"0\"  \/>\n                    <\/div>\n                    <div class=\"qty-right\">\n                      <button class=\"uparrow qtyplus\"  field='updates_{{ variant.id }}' >+<\/button>\n                      <button class=\"downarrow qtyminus\"  field='updates_{{ variant.id }}'>-<\/button>\n                    <\/div>\n                  <\/div>\n                  {%endcomment%}\n                  \n                  \n\t\t\t\t{%include 'quantity-selector' %} \n                  \n                {%if variant.metafields.custom_fields[\"warning_symbol\"] == 1 %}                      \n                <div class=\"exclamation-mark\">\n                  <img src=\"{{ 'Prop_65_Warning_Icon_small.png' | asset_img_url: '18x' }}\">\n                <\/div>\n                {%endif%}\n                {% if variant.inventory_management == 'shopify' and variant.inventory_policy == 'deny' and variant.inventory_quantity == 0 %}\n              \n                <div class=\"\">Out of Stock&nbsp;&nbsp;<a class=\"klaviyo-bis-trigger\" href=\"#\" style=\"display:block!important\" data=\"{{ variant.id }}\" onclick=\"setTimeout( function(){ $('#klaviyo-bis-iframe').contents().find('#variants').val({{variant.id}}); $('#klaviyo-bis-iframe').contents().find('#variants').attr('disabled',true);$('#klaviyo-bis-iframe').contents().find('#variants').css({'webkit-appearance':'none','-moz-appearance':'none'});}  , 1000 );\">Notify Me<\/a><\/div>\n                \n                {% endif %}\n              <\/td>\n            <\/tr> \n            {%endfor%} \n          <\/tbody>\n        <\/table>         \n\n        <div class=\"mobile-variant-table\">\n          {% for variant in product.variants %} \n                <div class=\"variant-contents mob-variant-contents\">\n              <div class=\"variant-row\">\n                <div class=\"variant-item\">\n                  <h4>{{product.title}}<\/h4>\n                  <h2>{{t}}<\/h2>\n                <\/div>\n              <\/div>\t\n              <div class=\"variant-row varient-custom\">\n                <div class=\"variant-item varient_size\">\n                  <label>Size<\/label>\n                  <span class=\"size\">{{ variant.option1}}<\/span>\n                <\/div>\n                <div class=\"variant-item varient_width\">\n                  <label>Profile<\/label>\n                  <span class=\"width\">{{ variant.metafields.custom_fields[\"profile\"] }}<\/span>\n                <\/div>\n              <!--  <div class=\"variant-item varient_length\">\n                  <label>Length<\/label>\n                  <span class=\"length\">{{ variant.metafields.custom_fields[\"length\"] }}<\/span>\n                <\/div>-->\n              <\/div>\t\n              <div class=\"variant-row varient-custom\">\n                <div class=\"variant-item varient_msrp\">\n                  <label>MSRP<\/label>\n                  <span class=\"msrp\">${{ variant.metafields.custom_fields[\"msrp\"] }}<\/span>\n                <\/div>\n              <\/div>\n              <div class=\"variant-row\">\n                <div class=\"variant-item\">\n                  <div class=\"variant-row\">\n                    <div class=\"variant-item varient_price\">\n                      <label>Price<\/label>\n                           {%- capture wsprice -%}{% include 'ws-item-price', item: variant, money: false %}{%- endcapture -%}{%- assign wsprice = wsprice | plus: 0 -%}\n         \n                      <span class=\"price\">{{ wsprice | money }}<\/span>\n                    <\/div>\n                  <\/div>\n                <\/div>\n                <div class=\"variant-item_number\">\n                  <div class=\"qty-wrapper\">\n                    <div class=\"qty-right\">\n                      <button class=\"uparrow qtyplus\"  field='updates_{{ variant.id }}' >+<\/button>\n                      <div class=\"span-absolute\"><\/div>\n                      <input  {% if variant.inventory_management == 'shopify' and variant.inventory_policy == 'deny' %}data-max=\"{{ variant.inventory_quantity }}\"{% endif %} data-price=\"{{ wsprice | money }}\" type=\"text\" name=\"updates[]\" field='{{ variant.id }}'  id=\"updates_{{ variant.id }}\" class=\"stickyquantity\" value=\"0\"  \/>\n                      <button class=\"downarrow qtyminus\"  field='updates_{{ variant.id }}'>-<\/button>\n                    <\/div>\n                  <\/div>\n                  {%if variant.metafields.custom_fields[\"warning_symbol\"] == 1 %}                      \n                  <div class=\"exclamation-mark\">\n                    <img src=\"{{ 'Prop_65_Warning_Icon_small.png' | asset_img_url: '18x' }}\">\n                  <\/div>\n                  {%endif%}\n                  {% if variant.inventory_management == 'shopify' and variant.inventory_policy == 'deny' and variant.inventory_quantity == 0 %}\n                 \n                  <div class=\"out-stock\">Out of Stock&nbsp;&nbsp;<a class=\"klaviyo-bis-trigger\" href=\"#\" style=\"display:block!important\" data=\"{{ variant.id }}\" onclick=\"setTimeout( function(){ $('#klaviyo-bis-iframe').contents().find('#variants').val({{variant.id}}); $('#klaviyo-bis-iframe').contents().find('#variants').attr('disabled',true);$('#klaviyo-bis-iframe').contents().find('#variants').css({'webkit-appearance':'none','-moz-appearance':'none'});}  , 1000 );\">Notify Me<\/a><\/div>\n                  \n                 {% endif %}\n                <\/div>\n              <\/div>   \n            <\/div>\n          {%endfor%}\n        <\/div>\n      <\/div>      \n    <\/div>\n  <\/div>\n<\/div><\/div>",
